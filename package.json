{
  "name": "ac-autocomplete",
  "displayName": "AC AutoComplete",
  "description": "An example VS Code extension for autocomplete",
  "version": "1.0.0",
  "publisher": "AC AutoComplete for Team Broker",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ac-autocomplete.helloWorld"
  ],
  "main": "./extension.js",
  "contributes": {
    "snippets": [
    {
      "language": "json",  
      "path": "snippets/vc_snippet.json"  
    },
    {
      "language": "json",  
      "path": "snippets/cm_snippet.json"  
    },
    {
      "language": "json",  
      "path": "snippets/cd_snippet.json"  
    },
  
    {
      "language": "json",  
      "path": "snippets/pt_snippet.json"  
    },
    {
      "language": "json",  
      "path": "snippets/mr_snippet.json"  
    },
    {
      "language": "json",  
      "path": "snippets/rf_snippet.json"  
    },
    {
      "language": "json",  
      "path": "snippets/rs_snippet.json"  
    }
  ],
    "commands": [
      {
        "command": "ac-autocomplete.helloWorld",
        "title": "Hello World"
      }
    ]
  }
}
